  <widget class="Gtk.Window" id="MainWindow" design-size="739 300">
    <action-group name="services">
      <action id="ActionServiceTikets">
        <property name="Type">Action</property>
        <property name="Label" translatable="yes">Журнал заявок</property>
        <property name="ShortLabel" translatable="yes">Журнал заявок</property>
        <property name="StockId">table</property>
      </action>
    </action-group>
    <action-group name="Default">
      <action id="Action">
        <property name="Type">Action</property>
        <property name="Label" translatable="yes">База</property>
        <property name="ShortLabel" translatable="yes">База</property>
      </action>
      <action id="dialogAuthenticationAction">
        <property name="Type">Action</property>
        <property name="Label" translatable="yes">Изменить пароль</property>
        <property name="ShortLabel" translatable="yes">Изменить пароль</property>
        <property name="StockId">gtk-dialog-authentication</property>
        <signal name="Activated" handler="OnDialogAuthenticationActionActivated" />
      </action>
      <action id="Action1">
        <property name="Type">Action</property>
        <property name="Label" translatable="yes">Справка</property>
        <property name="ShortLabel" translatable="yes">Справка</property>
      </action>
      <action id="Action2">
        <property name="Type">Action</property>
        <property name="Label" translatable="yes">Справочники</property>
        <property name="ShortLabel" translatable="yes">Справочники</property>
      </action>
      <action id="aboutAction">
        <property name="Type">Action</property>
        <property name="Label" translatable="yes">_О программе</property>
        <property name="ShortLabel" translatable="yes">_О программе</property>
        <property name="StockId">gtk-about</property>
        <signal name="Activated" handler="OnAboutActionActivated" />
      </action>
      <action id="UsersAction">
        <property name="Type">Action</property>
        <property name="Label" translatable="yes">Пользователи</property>
        <property name="ShortLabel" translatable="yes">Пользователи</property>
        <property name="StockId">users</property>
        <signal name="Activated" handler="OnAction3Activated" />
      </action>
      <action id="quitAction">
        <property name="Type">Action</property>
        <property name="Label" translatable="yes">В_ыход</property>
        <property name="ShortLabel" translatable="yes">В_ыход</property>
        <property name="StockId">gtk-quit</property>
        <signal name="Activated" handler="OnQuitActionActivated" />
      </action>
      <action id="ActionOrders">
        <property name="Type">Radio</property>
        <property name="Label" translatable="yes">Заказы</property>
        <property name="ShortLabel" translatable="yes">Заказы</property>
        <property name="StockId">order</property>
        <property name="DrawAsRadio">False</property>
        <property name="Active">False</property>
        <property name="Value">0</property>
        <property name="Group">group1</property>
        <signal name="Toggled" handler="OnActionOrdersToggled" />
      </action>
      <action id="ActionServices">
        <property name="Type">Radio</property>
        <property name="Label" translatable="yes">Сервис</property>
        <property name="ShortLabel" translatable="yes">Сервис</property>
        <property name="StockId">service</property>
        <property name="DrawAsRadio">False</property>
        <property name="Active">False</property>
        <property name="Value">0</property>
        <property name="Group">group1</property>
        <signal name="Toggled" handler="OnActionServicesToggled" />
      </action>
      <action id="ActionLogistics">
        <property name="Type">Radio</property>
        <property name="Label" translatable="yes">Логистика</property>
        <property name="ShortLabel" translatable="yes">Логистика</property>
        <property name="StockId">logistic</property>
        <property name="DrawAsRadio">False</property>
        <property name="Active">False</property>
        <property name="Value">0</property>
        <property name="Group">group1</property>
      </action>
      <action id="ActionStock">
        <property name="Type">Radio</property>
        <property name="Label" translatable="yes">Склад</property>
        <property name="ShortLabel" translatable="yes">Склад</property>
        <property name="StockId">stock</property>
        <property name="DrawAsRadio">False</property>
        <property name="Active">False</property>
        <property name="Value">0</property>
        <property name="Group">group1</property>
      </action>
      <action id="ActionCash">
        <property name="Type">Radio</property>
        <property name="Label" translatable="yes">Касса</property>
        <property name="ShortLabel" translatable="yes">Касса</property>
        <property name="StockId">cash</property>
        <property name="DrawAsRadio">False</property>
        <property name="Active">False</property>
        <property name="Value">0</property>
        <property name="Group">group1</property>
      </action>
      <action id="ActionAccounting">
        <property name="Type">Radio</property>
