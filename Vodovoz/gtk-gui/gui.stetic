        <property name="StockId">accounting</property>
        <property name="DrawAsRadio">False</property>
        <property name="Active">False</property>
        <property name="Value">0</property>
        <property name="Group">group1</property>
      </action>
      <action id="ActionReports">
        <property name="Type">Radio</property>
        <property name="Label" translatable="yes">Отчеты</property>
        <property name="ShortLabel" translatable="yes">Отчеты</property>
        <property name="StockId">report</property>
        <property name="DrawAsRadio">False</property>
        <property name="Active">False</property>
        <property name="Value">0</property>
        <property name="Group">group1</property>
      </action>
      <action id="ActionArchive">
        <property name="Type">Radio</property>
        <property name="Label" translatable="yes">Архив</property>
        <property name="ShortLabel" translatable="yes">Архив</property>
        <property name="StockId">archive</property>
        <property name="DrawAsRadio">False</property>
        <property name="Active">False</property>
        <property name="Value">0</property>
        <property name="Group">group1</property>
      </action>
    </action-group>
    <action-group name="orders">
      <action id="ActionNewOrder">
        <property name="Type">Action</property>
        <property name="Label" translatable="yes">Новый заказ</property>
        <property name="ShortLabel" translatable="yes">Новый заказ</property>
        <property name="StockId">gtk-new</property>
      </action>
      <action id="ActionOrdersTable">
        <property name="Type">Action</property>
        <property name="Label" translatable="yes">Журнал заказов</property>
        <property name="ShortLabel" translatable="yes">Журнал заказов</property>
        <property name="StockId">table</property>
      </action>
    </action-group>
    <property name="MemberName" />
    <property name="Title" translatable="yes">MainWindow</property>
    <property name="WindowPosition">CenterOnParent</property>
    <signal name="DeleteEvent" handler="OnDeleteEvent" />
    <child>
      <widget class="Gtk.VBox" id="vbox1">
        <property name="MemberName" />
        <property name="Spacing">6</property>
        <child>
          <widget class="Gtk.MenuBar" id="menubarMain">
            <property name="MemberName" />
            <node name="menubarMain" type="Menubar">
              <node type="Menu" action="Action">
                <node type="Menuitem" action="dialogAuthenticationAction" />
                <node type="Menuitem" action="UsersAction" />
                <node type="Separator" />
                <node type="Menuitem" action="quitAction" />
              </node>
              <node type="Menu" action="Action2" />
              <node type="Menu" action="Action1">
                <node type="Menuitem" action="aboutAction" />
              </node>
            </node>
          </widget>
          <packing>
            <property name="Position">0</property>
            <property name="AutoSize">True</property>
            <property name="Expand">False</property>
            <property name="Fill">False</property>
          </packing>
        </child>
        <child>
          <widget class="Gtk.Toolbar" id="toolbarMain">
            <property name="MemberName" />
            <property name="ShowArrow">False</property>
            <property name="ButtonStyle">Both</property>
            <property name="IconSize">Dialog</property>
            <node name="toolbarMain" type="Toolbar">
              <node type="Toolitem" action="ActionOrders" />
              <node type="Toolitem" action="ActionServices" />
              <node type="Toolitem" action="ActionLogistics" />
              <node type="Toolitem" action="ActionStock" />
              <node type="Toolitem" action="ActionCash" />
              <node type="Toolitem" action="ActionAccounting" />
              <node type="Toolitem" action="ActionReports" />
              <node type="Toolitem" action="ActionArchive" />
            </node>
          </widget>
          <packing>
            <property name="Position">1</property>
            <property name="AutoSize">True</property>
            <property name="Expand">False</property>
            <property name="Fill">False</property>
          </packing>
        </child>
        <child>
          <widget class="Gtk.Toolbar" id="toolbarSub">
            <property name="MemberName" />
            <property name="ShowArrow">False</property>
            <property name="ButtonStyle">Text</property>
            <property name="IconSize">SmallToolbar</property>
            <node name="toolbarSub" type="Toolbar" />
          </widget>
          <packing>
            <property name="Position">2</property>
            <property name="AutoSize">True</property>
            <property name="Expand">False</property>
            <property name="Fill">False</property>
          </packing>
        </child>
        <child>
          <placeholder />
        </child>
        <child>
          <widget class="Gtk.Statusbar" id="statusbarMain">
            <property name="MemberName" />
            <property name="Spacing">6</property>
            <child>
              <widget class="Gtk.Label" id="labelUser">
                <property name="MemberName" />
                <property name="LabelProp" translatable="yes">label2</property>
              </widget>
              <packing>
                <property name="Position">0</property>
                <property name="AutoSize">True</property>
                <property name="Expand">False</property>
                <property name="Fill">False</property>
              </packing>
            </child>
            <child>
              <widget class="Gtk.Label" id="labelStatus">
                <property name="MemberName" />
                <property name="LabelProp" translatable="yes">label1</property>
              </widget>
              <packing>
                <property name="Position">2</property>
                <property name="AutoSize">True</property>
                <property name="Expand">False</property>
                <property name="Fill">False</property>
              </packing>
            </child>
            <child>
              <placeholder />
            </child>
          </widget>
          <packing>
            <property name="PackType">End</property>
            <property name="Position">4</property>
            <property name="AutoSize">True</property>
            <property name="Expand">False</property>
            <property name="Fill">False</property>
          </packing>
        </child>
      </widget>
    </child>
  </widget>
</stetic-interface>