  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
	nomenclature.name AS nomenclature,
	nomenclature.category,
	measurement_units.name AS unit,
	nomenclature.tare_volume,
	CASE nomenclature.tare_volume
		WHEN 'Vol19L' THEN 0
		WHEN 'Vol6L' THEN 1
		WHEN 'Vol600ml' THEN 2
		ELSE 3
	END AS clmn_for_sorting,
	SUM(order_items.count) AS amount
FROM
	route_list_addresses
		LEFT JOIN
	order_items ON route_list_addresses.order_id = order_items.order_id
		LEFT JOIN
	nomenclature ON order_items.nomenclature_id = nomenclature.id
		LEFT JOIN
	measurement_units ON measurement_units.id = nomenclature.unit_id
		LEFT JOIN 
	nomenclature_groups ng ON ng.id = nomenclature.group_id
		LEFT JOIN
	nomenclatures_to_warehouses ON nomenclatures_to_warehouses.nomenclature_id = nomenclature.id
WHERE
	route_list_addresses.route_list_id = @id
		AND nomenclatures_to_warehouses.warehouse_id = 1 #Отгрузка только с софийской
		AND ((route_list_addresses.was_transfered = true AND route_list_addresses.need_to_reload = true) OR route_list_addresses.was_transfered = false)
		AND IF(nomenclature.group_id IS NOT NULL, ng.is_online_store = FALSE, TRUE) 
GROUP BY nomenclature.id
ORDER BY clmn_for_sorting;</CommandText>
        <QueryParameters>
          <QueryParameter Name="id">
            <Value>={?route_list_id}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="nomenclature">
          <DataField>nomenclature</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="category">
          <DataField>category</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="unit">
          <DataField>unit</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="tare_volume">
          <DataField>tare_volume</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="amount">
          <DataField>amount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Doc">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
	route_lists.id AS route_list_id,
	route_lists.date AS route_list_date,
	CONCAT_WS('',
			CONCAT(drivers.last_name, ' '),
			IF(drivers.name = '',
				'',
				CONCAT(LEFT(drivers.name, 1), '.')),
			IF(drivers.patronymic = '',
				'',
				CONCAT(LEFT(drivers.patronymic, 1), '.'))) AS driver,
	CONCAT_WS('',
			CONCAT(forwaders.last_name, ' '),
			IF(forwaders.name = '',
				'',
				CONCAT(LEFT(forwaders.name, 1), '.')),
			IF(forwaders.patronymic = '',
				'',
				CONCAT(LEFT(forwaders.patronymic, 1), '.'))) AS forwader,
	cars.model AS model,
	cars.reg_number AS reg_number
FROM
	route_lists
		LEFT JOIN
	employees AS drivers ON drivers.id = route_lists.driver_id
		LEFT JOIN
	employees AS forwaders ON forwaders.id = route_lists.forwarder_id
		LEFT JOIN
	cars ON cars.id = route_lists.car_id
WHERE
	route_lists.id = @id
GROUP BY route_lists.id
LIMIT 1
;</CommandText>
        <QueryParameters>
          <QueryParameter Name="@id">
            <Value>={?route_list_id}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="route_list_id">
          <DataField>route_list_id</DataField>
          <TypeName>System.UInt32</TypeName>
        </Field>
        <Field Name="route_list_date">
          <DataField>route_list_date</DataField>
          <TypeName>System.DateTime</TypeName>
        </Field>
        <Field Name="driver">
          <DataField>driver</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="forwader">
          <DataField>forwader</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="model">
          <DataField>model</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="reg_number">
          <DataField>reg_number</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Equipment">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
	n.official_name,
	SUM(eq.count) AS count,
	eq.order_id AS order_id
FROM(
	SELECT
		nomenclature_id,
		e.order_id,
		count
	FROM order_equipment e
	LEFT JOIN route_list_addresses a ON e.order_id = a.order_id
	WHERE a.route_list_id = @id 
		AND direction = 'Deliver'
		AND (a.was_transfered = true AND a.need_to_reload = true OR a.was_transfered = false)
	UNION ALL
	SELECT
		nomenclature_id,
		i.order_id,
		count
	FROM order_items i
	LEFT JOIN route_list_addresses a ON i.order_id = a.order_id
	WHERE a.route_list_id = @id
	AND (a.was_transfered = true AND a.need_to_reload = true OR a.was_transfered = false)
) AS eq
LEFT JOIN nomenclature n ON eq.nomenclature_id = n.id
LEFT JOIN nomenclature_groups ng ON ng.id = n.group_id
WHERE n.category IN ('equipment', 'additional', 'spare_parts', 'material')
AND IF(n.group_id IS NOT NULL, ng.is_online_store = FALSE, TRUE)
GROUP BY eq.nomenclature_id
;
</CommandText>
        <QueryParameters>
          <QueryParameter Name="id">
            <Value>={?route_list_id}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields >
        <Field Name="official_name">
          <DataField>official_name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="count">
          <DataField>count</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="order_id">
          <DataField>order_id</DataField>
          <TypeName>System.UInt32</TypeName>
        </Field>
      </Fields>
    </DataSet>
