  <DataSets>
    <DataSet Name="Complaint">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    complaints.id,
    CASE
        WHEN complaints.type = 'Inner' THEN 'ВН'
        WHEN complaints.type = 'Client' THEN 'КЛ'
    END AS complaint_type,
    complaints.creation_date,
    CASE
        WHEN complaints.status = 'InProcess' THEN 'В работе'
        WHEN complaints.status = 'Checking' THEN 'На проверке'
        WHEN complaints.status = 'Closed' THEN 'Закрыт'
    END AS complaint_status,
    GROUP_CONCAT(DISTINCT subdivisions.name SEPARATOR ' ') AS subdivisions,
    complaints.planned_completion_date,
    CONCAT_WS(', ', counterparty.name, COMPILE_ADDRESS(delivery_points.id)) AS client,
    CASE
        WHEN complaint_guilty_items.guilty_type = 'Client' THEN 'Клиент'
        WHEN complaint_guilty_items.guilty_type = 'Node' THEN 'Нет'
        WHEN complaint_guilty_items.guilty_type = 'Employee' THEN  CONCAT_WS('', CONCAT(guiltyEmployee.last_name, ' '), IF(guiltyEmployee.name = '', '', CONCAT(LEFT(guiltyEmployee.name, 1), '.')), IF(guiltyEmployee.patronymic = '', '', CONCAT(LEFT(guiltyEmployee.patronymic, 1), '.')))
        WHEN complaint_guilty_items.guilty_type = 'Subdivision' THEN guiltySubdivisions.name
    END AS guilties,
    complaints.complaint_text,
    CONCAT_WS('', CONCAT(employees.last_name, ' '), IF(employees.name = '', '', CONCAT(LEFT(employees.name, 1), '.')), IF(employees.patronymic = '', '', CONCAT(LEFT(employees.patronymic, 1), '.'))) AS creator,
    GROUP_CONCAT(DISTINCT ROUND(fines.total_money,2) SEPARATOR ' ') AS fines,
    complaints.result_text,
    complaints.actual_completion_date,
    DATEDIFF(complaints.actual_completion_date, complaints.creation_date) AS days_at_work
FROM
    complaints
    LEFT JOIN complaint_discussions ON complaint_discussions.complaint_id = complaints.id
    LEFT JOIN subdivisions ON subdivisions.id = complaint_discussions.subdivision_id
    LEFT JOIN counterparty ON counterparty.id = complaints.counterparty_id
    LEFT JOIN orders ON orders.id = complaints.order_id
    LEFT JOIN delivery_points ON delivery_points.id = orders.delivery_point_id
    LEFT JOIN complaint_guilty_items ON complaint_guilty_items.complaint_id = complaints.id
    LEFT JOIN employees AS guiltyEmployee ON guiltyEmployee.id = complaint_guilty_items.employee_id
    LEFT JOIN subdivisions AS guiltySubdivisions ON guiltySubdivisions.id = complaint_guilty_items.subdivision_id
    LEFT JOIN employees ON employees.id = complaints.created_by_id
    LEFT JOIN complaint_fines ON complaint_fines.complaint_id = complaints.id
    LEFT JOIN fines ON complaint_fines.fine_id = fines.id
WHERE
    (
        (
            @subdivision_id = 0 OR (@subdivision_id = 1
            AND (@start_date IS NULL OR complaints.planned_completion_date &gt;= @start_date)
            AND (@end_date IS NULL OR complaints.planned_completion_date &lt;= @end_date))
        )
        OR EXISTS
        (
            SELECT
                *
            FROM 
                complaint_discussions as disc
            WHERE
                disc.complaint_id = complaints.id
                AND disc.subdivision_id = @subdivision_id
                AND (@start_date IS NULL OR disc.planned_completion_date &gt;= @start_date)
                AND (@end_date IS NULL OR disc.planned_completion_date &lt;= @end_date)
        )
    )
    AND (@employee_id = 0 OR complaints.created_by_id = @employee_id)
    AND (@status = '' OR complaints.status = @status)
    AND (@type = '' OR complaints.type = @type)
GROUP BY
    complaints.id</CommandText>
        <QueryParameters>
          <QueryParameter Name="start_date">
            <Value>={?start_date}</Value>
          </QueryParameter>
          <QueryParameter Name="end_date">
            <Value>={?end_date}</Value>
          </QueryParameter>
          <QueryParameter Name="status">
            <Value>={?status}</Value>
          </QueryParameter>
          <QueryParameter Name="type">
            <Value>={?type}</Value>
          </QueryParameter>
          <QueryParameter Name="subdivision_id">
            <Value>={?subdivision_id}</Value>
          </QueryParameter>
          <QueryParameter Name="employee_id">
            <Value>={?employee_id}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields >
        <Field Name="id">
          <DataField>id</DataField>
        </Field>
        <Field Name="complaint_type">
          <DataField>complaint_type</DataField>
        </Field>
