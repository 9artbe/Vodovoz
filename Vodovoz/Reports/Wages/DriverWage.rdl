  <DataSets>
    <DataSet Name="DriverInfo">
      <Query>
        <DataSourceName>
        </DataSourceName>
        <CommandText>SELECT 
    route_lists.date AS date,
    route_lists.id AS route_list_number,
    COUNT(route_list_addresses.id) AS addresses_count,
    SUM(order_items.actual_count) AS full_bottles,
    SUM(route_list_addresses.bottles_returned) AS empty_bottles,
	SUM(route_list_addresses.driver_wage) AS wage
FROM
    route_lists
        LEFT JOIN
    route_list_addresses ON route_list_addresses.route_list_id = route_lists.id
        LEFT JOIN
    orders ON orders.id = route_list_addresses.order_id
        LEFT JOIN
    order_items ON order_items.order_id = orders.id
        LEFT JOIN
    nomenclature ON nomenclature.id = order_items.nomenclature_id
WHERE
    route_lists.driver_id = @driver_id
        AND date &gt; @start_date
        AND date &lt; @end_date
        AND (route_lists.status = 'OnClosing'
        OR route_lists.status = 'NotDelivered'
        OR route_lists.status = 'MileageCheck'
        OR route_lists.status = 'Closed')
        AND (nomenclature.category = 'bottle'
        OR nomenclature.category = 'water'
        OR nomenclature.category = 'disposableBottleWater')
GROUP BY route_lists.id;</CommandText>
        <QueryParameters>
          <QueryParameter Name="driver_id">
            <Value>=Parameters!driver_id</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="date">
          <DataField>date</DataField>
          <TypeName>System.DateTime</TypeName>
        </Field>
        <Field Name="route_list_number">
          <DataField>route_list_number</DataField>
          <TypeName>System.Int32</TypeName>
        </Field>
        <Field Name="addresses_count">
          <DataField>addresses_count</DataField>
          <TypeName>System.Int32</TypeName>
        </Field>
        <Field Name="full_bottles">
          <DataField>full_bottles</DataField>
          <TypeName>System.Int32</TypeName>
        </Field>
        <Field Name="empty_bottles">
          <DataField>empty_bottles</DataField>
          <TypeName>System.Int32</TypeName>
        </Field>
        <Field Name="wage">
          <DataField>wage</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DriverName">
      <Query>
