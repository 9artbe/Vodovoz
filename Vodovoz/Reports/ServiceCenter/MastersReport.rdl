  <DataSets>
    <DataSet Name="data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>select
	route_lists.date,
	concat(employees.last_name, ' ', employees.name) as driver,
	entries.*
from
	route_lists
right join
	(select * from 
 (
     select 
        innerTab.rl_id,
        innerTab.entry_name,
        innerTab.entry_address,
        innerTab.entry_price,
        innerTab.entry_count,
        innerTab.entry_payment_type,
        innerTab.entry_sum,
        innerTab.entry_wage,
        innerTab.entry_profit,
        innerTab.entry_advance
       from
        (select
                delivery_points.id,
                route_list_addresses.route_list_id as rl_id,
                counterparty.name as entry_name,
                delivery_points.compiled_address as entry_address,
                order_items.price as entry_price,
                  order_items.count as entry_count,
                  orders.payment_type as entry_payment_type,
                  sum(order_items.price * order_items.count) as entry_sum,
                  route_list_addresses.driver_wage as entry_wage,
                 sum(order_items.price * order_items.count - route_list_addresses.driver_wage) as entry_profit
                 , 0 as entry_advance
            from
                route_list_addresses
            left join
                orders on orders.id = route_list_addresses.order_id
            left join
                counterparty on counterparty.id = orders.client_id
            left join
                delivery_points on delivery_points.id = orders.delivery_point_id
            left join
                order_items on order_items.order_id = orders.id
            left join
                nomenclature on nomenclature.id = order_items.nomenclature_id
            left join
                order_equipment on order_equipment.order_id = orders.id
            where
                nomenclature.category in ('master', 'service')
                #Исключатся недовозы, частичные недовозы и отмены
                AND (
                    (orders.order_status NOT IN ('NotDelivered', 'Canceled'))  
                    OR NOT (
                        # Частичные недовозы
                        route_list_addresses.status = 'Completed' 
                        AND                     
                        (
                            nomenclature.category in ('master', 'service')
                            AND nomenclature.serial = false                 
                            AND order_items.count != order_items.actual_count
                        )
                        OR order_equipment.confirmed = false    
                    )
                )
           group by delivery_points.id, route_list_addresses.route_list_id
                ) innerTab
     union all
        select
         cash_expense.route_list_id as rl_id,
            cash_expense.description as entry_name,
            '' as entry_address,
            0 as entry_price,
            0 as entry_count,
            0 as entry_payment_type,
            0 as entry_sum,
            0 as entry_wage,
            0 as entry_profit,
            cash_expense.money as entry_advance
        from
            cash_expense
        where
            cash_expense.money  &lt;&gt; 0 
    ) innerExpense ) as entries on entries.rl_id = route_lists.id
left join
	employees on employees.id = route_lists.driver_id
where
	route_lists.date &gt;= @start_date
	and route_lists.date &lt; date_add(@end_date, interval 1 day)
	and route_lists.driver_id = @driver_id
order by route_lists.date, route_lists.id</CommandText>
