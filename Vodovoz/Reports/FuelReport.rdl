  <DataSets>
    <DataSet Name="Fuel">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
    outlayed.date AS date,
    route_lists.id AS route_list_id,
    CONCAT_WS(' ', employees.last_name, employees.name) AS driver,
    delivery_shift.name AS shift,
    route_lists.actual_distance AS distance,
    outlayed.liters_outlayed AS liters_outlayed,
    gived.liters_gived AS liters_gived,
    @fuel_balance:=@fuel_balance - IF(outlayed.liters_outlayed IS NULL,
        0,
        outlayed.liters_outlayed) + IF(gived.liters_gived IS NULL,
        0,
        gived.liters_gived) AS balance,
    fuel_documents.payed_for_fuel AS payed_for_fuel
FROM
    (SELECT 
        SUM(@fuel_balance:=@fuel_balance + IF(internal.liters_gived IS NULL, 0, internal.liters_gived) - IF(internal.liters_outlayed IS NULL, 0, internal.liters_outlayed)) AS rows_first
    FROM
        fuel_operations AS internal, (SELECT @fuel_balance:=0) t
    WHERE
        internal.date &lt; @start_date
            AND (IF(@driver_id IS NULL, internal.car_id = @car_id, internal.driver_id = @driver_id))) t1,
    fuel_operations outlayed
        LEFT JOIN
    route_lists ON route_lists.fuel_outlayed_operation_id = outlayed.id
        LEFT JOIN
    fuel_documents ON route_lists.fuel_gived_document_id = fuel_documents.id
        LEFT JOIN
    fuel_operations AS gived ON fuel_documents.fuel_operation_id = gived.id
        LEFT JOIN
    delivery_shift ON route_lists.delivery_shift_id = delivery_shift.id
        LEFT JOIN
    employees ON route_lists.driver_id = employees.id
WHERE
    outlayed.date &gt;= @start_date
        AND outlayed.date &lt;= @end_date
        AND (IF(@driver_id IS NULL,
	outlayed.car_id = @car_id,
	outlayed.driver_id = @driver_id))
        AND outlayed.liters_outlayed &gt; 0</CommandText>
        <QueryParameters>
          <QueryParameter Name="start_date">
            <Value>=Parameters!start_date</Value>
          </QueryParameter>
          <QueryParameter Name="end_date">
            <Value>=Parameters!end_date</Value>
          </QueryParameter>
          <QueryParameter Name="car_id">
            <Value>=Parameters!car_id</Value>
          </QueryParameter>
          <QueryParameter Name="driver_id">
            <Value>=Parameters!driver_id</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="date">
          <DataField>date</DataField>
          <TypeName>System.DateTime</TypeName>
        </Field>
        <Field Name="route_list_id">
          <DataField>route_list_id</DataField>
          <TypeName>System.Int32</TypeName>
        </Field>
        <Field Name="driver">
          <DataField>driver</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="shift">
          <DataField>shift</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="distance">
          <DataField>distance</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="liters_outlayed">
          <DataField>liters_outlayed</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="liters_gived">
          <DataField>liters_gived</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="balance">
          <DataField>balance</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="payed_for_fuel">
          <DataField>payed_for_fuel</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="StartBalance" >
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
    SUM(IF(internal.liters_gived IS NULL, 0, internal.liters_gived)
      - IF(internal.liters_outlayed IS NULL, 0, internal.liters_outlayed)
      ) AS balance
FROM
    fuel_operations AS internal
WHERE
    internal.date &lt; @start_date
        AND (IF(@driver_id IS NULL,
        internal.car_id = @car_id,
        internal.driver_id = @driver_id))</CommandText>
        <QueryParameters>
          <QueryParameter Name="start_date">
            <Value>=Parameters!start_date</Value>
          </QueryParameter>
          <QueryParameter Name="car_id">
            <Value>=Parameters!car_id</Value>
          </QueryParameter>
          <QueryParameter Name="driver_id">
            <Value>=Parameters!driver_id</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="balance">
          <DataField>balance</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
      </Fields>
    </DataSet>
