    <DataSet Name="Warehouses">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
    (SELECT 
            name
        FROM
            warehouses
        WHERE
            id = @warehouse1_id) AS warehouse1,
    (SELECT 
            name
        FROM
            warehouses
        WHERE
            id = @warehouse2_id) AS warehouse2,
    (SELECT 
            name
        FROM
            warehouses
        WHERE
            id = @warehouse3_id) AS warehouse3,
    (SELECT 
            name
        FROM
            warehouses
        WHERE
            id = @warehouse4_id) AS warehouse4</CommandText>
