            <Value>=Parameters!warehouse3_id</Value>
          </QueryParameter>
          <QueryParameter Name="warehouse4_id">
            <Value>=Parameters!warehouse4_id</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
