  <DataSets>
    <DataSet Name="data">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
	T.order_id,
	T.order_author,
	T.client,
	SUM(T.sum_cash) AS sum_cash,
	SUM(T.water_count) AS water_count,
	T.tare_to_collect,
	T.status,
	T.geo_group,
	T.rl_id,
	T.driver
FROM(
	SELECT
		orders.id AS order_id,
		CONCAT(IFNULL(SUBSTR(author.name,1,1),''),'.',IFNULL(author.last_name,'')) AS order_author,
		counterparty.full_name AS client,
		TRUNCATE(order_items.count * order_items.price - order_items.discount_money, 2) AS sum_cash,
		IF(
			nomenclature.tare_volume = 'Vol19L',
			order_items.count,
			0
		) AS water_count,
		orders.bottles_return AS tare_to_collect,
		CASE orders.order_status
			WHEN 'NewOrder' THEN 'Новый'
			WHEN 'WaitForPayment' THEN 'Ожидание оплаты'
			WHEN 'Accepted' THEN 'Принят'
			WHEN 'InTravelList' THEN 'В маршрутном листе'
			WHEN 'OnLoading' THEN 'На погрузке'
			WHEN 'OnTheWay' THEN 'В пути'
			WHEN 'Shipped' THEN 'Доставлен'
			WHEN 'UnloadingOnStock' THEN 'Выгрузка на складе'
			WHEN 'Closed' THEN 'Закрыт'
			WHEN 'Canceled' THEN 'Отменён'
			WHEN 'NotDelivered' THEN 'Недовоз'
			WHEN 'DeliveryCanceled' THEN 'Доставка отменена'
			ELSE orders.order_status
		END AS status,
		GROUP_CONCAT(DISTINCT gg.name SEPARATOR ', ') AS geo_group,
		rl.id AS rl_id,
		IF(
			rl.id IS NOT NULL,
			CONCAT(
				IFNULL(SUBSTR(driver.name,1,1),''),
				'.',
				IFNULL(driver.last_name,'')
			),
			NULL
		) AS driver
	FROM
		orders
	LEFT JOIN employees author ON author.id = orders.author_employee_id
	JOIN order_items ON order_items.order_id = orders.id
	LEFT JOIN nomenclature ON nomenclature.id = order_items.nomenclature_id
	LEFT JOIN counterparty ON counterparty.id = orders.client_id
	LEFT JOIN delivery_points ON delivery_points.id = orders.delivery_point_id
	LEFT JOIN schedule_restricted_districts distr ON delivery_points.schedule_restricted_district_id = distr.id
	LEFT JOIN geographic_groups_to_entities gge ON gge.district_id = distr.id
	LEFT JOIN geographic_groups gg ON gge.geographic_group_id = gg.id
	LEFT JOIN route_list_addresses rli ON rli.order_id = orders.id AND rli.transfered_to_id IS NULL
	LEFT JOIN route_lists rl ON rli.route_list_id = rl.id
	LEFT JOIN employees driver ON driver.id = rl.driver_id
	WHERE
		orders.delivery_date = DATE(@date)
		AND ((orders.payment_type = 'cash' AND orders.need_cheque = 'Yes') OR orders.payment_type = 'BeveragesWorld')
		AND (gg.id IN(@geographic_groups) OR 0 IN(@geographic_groups))
	GROUP BY order_items.id
	) AS T
GROUP BY T.order_id
HAVING SUM(T.sum_cash) &gt; 0
;</CommandText>
        <QueryParameters>
          <QueryParameter Name="date">
            <Value>=Parameters!date</Value>
          </QueryParameter>
          <QueryParameter Name="geographic_groups">
            <Value>=Parameters!geographic_groups</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
