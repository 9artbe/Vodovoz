  <DataSets>
    <DataSet Name="Orders">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
    orders.id AS order_id,
    counterparty.full_name AS counterparty,
    delivery_points.compiled_address_short AS address,
    CONCAT_WS(&#39; &#39;, employees.last_name, employees.name) AS driver_name,
    orders.comment AS comment,
    (CAST(delivered AS SIGNED) - CAST(returned AS SIGNED)) AS bottles
FROM
    orders
        LEFT JOIN
    counterparty ON counterparty.id = orders.client_id
        LEFT JOIN
    delivery_points ON delivery_points.id = orders.delivery_point_id
        LEFT JOIN
    route_list_addresses ON route_list_addresses.order_id = orders.id AND route_list_addresses.status &lt;&gt; &#39;Transfered&#39;
        LEFT JOIN
    route_lists ON route_lists.id = route_list_addresses.route_list_id
        LEFT JOIN
    employees ON employees.id = route_lists.driver_id
        LEFT JOIN
    bottles_movement_operations ON bottles_movement_operations.id = orders.bottles_movement_operation_id
WHERE
    orders.delivery_date = DATE(@date)
        AND (route_lists.status = &#39;Closed&#39;
        OR route_lists.status = &#39;MileageCheck&#39;)
        AND (CAST(delivered AS SIGNED) - CAST(returned AS SIGNED)) &gt; 0</CommandText>
        <QueryParameters>
          <QueryParameter Name="date">
            <Value>=Parameters!date</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="order_id">
          <DataField>order_id</DataField>
          <TypeName>System.UInt32</TypeName>
        </Field>
        <Field Name="counterparty">
          <DataField>counterparty</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="address">
          <DataField>address</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="driver_name">
          <DataField>driver_name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="comment">
          <DataField>comment</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="bottles">
          <DataField>bottles</DataField>
          <TypeName>System.Int32</TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="RouteLists">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
    route_lists.id AS route_list_id,
    CONCAT_WS(' ', employees.last_name, employees.name) AS driver_name,
    route_lists.status AS status
FROM
    route_lists
        LEFT JOIN
    employees ON employees.id = route_lists.driver_id
WHERE
    route_lists.date = @date
    AND (route_lists.status &lt;&gt; 'closed'
        AND route_lists.status &lt;&gt; 'MileageCheck')
    ORDER BY route_lists.status;</CommandText>
        <QueryParameters>
          <QueryParameter Name="date">
            <Value>=Parameters!date</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="route_list_id">
          <DataField>route_list_id</DataField>
          <TypeName>System.Int32</TypeName>
        </Field>
        <Field Name="driver_name">
          <DataField>driver_name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="status">
          <DataField>status</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
