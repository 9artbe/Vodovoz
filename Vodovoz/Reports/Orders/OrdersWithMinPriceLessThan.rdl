  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT DISTINCT
	counterparty.id AS client_id,
	counterparty.name AS client_name,
	delivery_points.id AS dpoint_id,
	IFNULL(delivery_points.compiled_address_short, delivery_points.compiled_address) AS dpoint_address,
	(
		SELECT
			MAX(orders.delivery_date)
		FROM
			orders
		WHERE
			orders.delivery_point_id = dpoint_id
	) AS last_order_date,
	(
		SELECT
			MIN(order_items.price)
		FROM
			orders
		LEFT JOIN
			order_items ON order_items.order_id = orders.id
		LEFT JOIN
			nomenclature ON nomenclature.id = order_items.nomenclature_id
		WHERE
				orders.delivery_point_id = dpoint_id
			AND
				nomenclature.category = 'water'
			AND
				order_items.price &gt; 0
			AND
				order_items.price BETWEEN 0.01 AND 129.99
	) AS min_price,
	(
		SELECT
			orders.delivery_date
		FROM
			orders
		LEFT JOIN
			order_items ON order_items.order_id = orders.id
		LEFT JOIN
			nomenclature ON nomenclature.id = order_items.nomenclature_id
		WHERE
				orders.delivery_point_id = dpoint_id
			AND
				nomenclature.category = 'water'
			AND
				order_items.price = min_price				
		LIMIT 1
	) AS min_price_date,
	(
		SELECT
			order_items.actual_count
		FROM
			orders
		LEFT JOIN
			order_items ON order_items.order_id = orders.id
		LEFT JOIN
			nomenclature ON nomenclature.id = order_items.nomenclature_id
		WHERE
				orders.delivery_point_id = dpoint_id
			AND
				nomenclature.category = 'water'
			AND
				order_items.price = min_price				
		LIMIT 1
	) AS min_price_quantity
FROM
	counterparty
LEFT JOIN
	delivery_points ON delivery_points.counterparty_id = counterparty.id
LEFT JOIN
	orders ON orders.client_id = counterparty.id
WHERE
	EXISTS
	(
		SELECT
			1
		FROM
			orders
		LEFT JOIN
			order_items ON order_items.order_id = orders.id
		LEFT JOIN
			nomenclature ON nomenclature.id = order_items.nomenclature_id
		WHERE
				orders.delivery_point_id = delivery_points.id
			AND
				nomenclature.category = 'water'
			AND
				order_items.price BETWEEN 0.01 AND 129.99
			AND 
				order_items.discount &lt; 100
	)</CommandText>
      </Query>
