  <DataSets>
    <DataSet Name="Orders">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT  
    delivery_date,
    counterparty.name AS counterparty,
    address_1c
FROM
    (SELECT 
        *
    FROM
        Vodovoz_honeybee.orders
    WHERE
        (orders.delivery_date = DATE(@date))
    ORDER BY delivery_date DESC) AS t2 
    LEFT JOIN counterparty ON t2.client_id = counterparty.id
 WHERE
   order_status &lt;&gt; ('Canceled' OR 'DeliveryCanceled')
GROUP BY t2.delivery_point_id</CommandText>
        <QueryParameters>
          <QueryParameter Name="date">
            <Value>=Parameters!date</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields >
        <Field Name="delivery_date">
          <DataField>delivery_date</DataField>
          <TypeName>System.DateTime</TypeName>
        </Field>
        <Field Name="counterparty">
          <DataField>counterparty</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="address_1c">
          <DataField>address_1c</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
    </DataSet>
