  <DataSets>
    <DataSet Name="Data">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
	o.id AS order_id,
	o.delivery_date AS delivery_date,
	c.full_name AS client_name,
	dp.compiled_address_short AS address_of_delivery,
	IFNULL(
		GROUP_CONCAT(
			DISTINCT
			CONCAT(dp_ph.number, ' ', IFNULL(dp_ph.name,''))
			SEPARATOR '\n'
		),
		GROUP_CONCAT(
			DISTINCT
			CONCAT(c_ph.number, ' ', IFNULL(c_ph.name,''))
			SEPARATOR '\n'
		)
	) AS phone,
	#GROUP_CONCAT(DISTINCT d.name SEPARATOR ', ') AS discount,
	(
		SELECT GROUP_CONCAT(DISTINCT d.name SEPARATOR ', ')
		FROM order_items i
		JOIN discount_reasons d ON i.discount_reason_id = d.id
		WHERE i.order_id = o.id
	) AS discount,
	o.comment AS comment
FROM orders o
JOIN order_items i ON o.id = i.order_id
JOIN discount_reasons d ON i.discount_reason_id = d.id
JOIN counterparty c ON o.id = c.first_order_id
JOIN delivery_points dp ON dp.id = o.delivery_point_id
LEFT JOIN phones dp_ph ON dp_ph.delivery_point_id = dp.id
LEFT JOIN phones c_ph ON c_ph.counterparty_id = c.id
WHERE o.delivery_date BETWEEN @start_date AND @end_date
AND (@discount_id = 0 OR d.id = @discount_id)
GROUP BY o.id DESC
;</CommandText>
        <QueryParameters>
          <QueryParameter Name="start_date">
            <Value>=Parameters!start_date</Value>
          </QueryParameter>
          <QueryParameter Name="end_date">
            <Value>=Parameters!end_date</Value>
          </QueryParameter>
          <QueryParameter Name="discount_id">
            <Value>=Parameters!discount_id</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields >
        <Field Name="order_id">
          <DataField>order_id</DataField>
          <TypeName>System.UInt32</TypeName>
        </Field>
        <Field Name="delivery_date">
          <DataField>delivery_date</DataField>
          <TypeName>System.DateTime</TypeName>
        </Field>
        <Field Name="client_name">
          <DataField>client_name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="address_of_delivery">
          <DataField>address_of_delivery</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="phone">
          <DataField>phone</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="discount">
          <DataField>discount</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="comment">
          <DataField>comment</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
    </DataSet>
