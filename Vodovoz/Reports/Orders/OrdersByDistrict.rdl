  <DataSets>
    <DataSet Name="Orders">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <Timeout>5</Timeout>
        <CommandText>SELECT   delivery_schedule.name as date, 
 order_items.count as bottles,  
 equipment_id,
 compiled_address as address,
 last_name  
 FROM  orders
 LEFT JOIN delivery_schedule on orders.delivery_schedule_id = delivery_schedule.id     
 LEFT JOIN delivery_points on delivery_point_id = delivery_points.id
 LEFT JOIN order_items on orders.id = order_items.order_id
 LEFT JOIN nomenclature on order_items.nomenclature_id = nomenclature.id
 LEFT JOIN equipment on order_items.equipment_id =equipment.id
 LEFT JOIN route_list_addresses on orders.id = route_list_addresses.order_id
 LEFT JOIN route_lists on route_list_addresses.route_list_id = route_lists.id
 LEFT JOIN employees on route_lists.driver_id = employees.id
 WHERE 
	nomenclature.weight = 20 
	AND  delivery_date BETWEEN @date_start  AND @date_end 
	AND ST_WITHIN(GEOMFROMTEXT(CONCAT('POINT(', latitude, ' ', longitude, ')')), (SELECT logistics_district  FROM logistics_area WHERE logistics_area.id = @id))</CommandText>
        <QueryParameters>
          <QueryParameter Name="id">
            <Value>=Parameters!id</Value>
          </QueryParameter>
          <QueryParameter Name="start_date">
            <Value>=Parameters!start_date</Value>
          </QueryParameter>
          <QueryParameter Name="end_date">
            <Value>=Parameters!end_date</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields >
        <Field Name="date">
          <DataField>date</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="bottles">
          <DataField>bottles</DataField>
          <TypeName>System.Int32</TypeName>
        </Field>
        <Field Name="equipment_id">
          <DataField>equipment_id</DataField>
          <TypeName>System.UInt32</TypeName>
        </Field>
        <Field Name="address">
          <DataField>address</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="last_name">
          <DataField>last_name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
