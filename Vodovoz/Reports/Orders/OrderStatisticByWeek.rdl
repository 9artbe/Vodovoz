  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
	T.week_begin,
	T.week_end,
	
	SUM(T.bottles_monday_day) as bottles_monday_day,
	COUNT(T.address_monday_day) as address_monday_day,
	SUM(T.bottles_monday_night) as bottles_monday_night,
	COUNT(T.address_monday_night) as address_monday_night,
	
	SUM(T.bottles_tuesday_day) as bottles_tuesday_day,
	COUNT(T.address_tuesday_day) as address_tuesday_day,
	SUM(T.bottles_tuesday_night) as bottles_tuesday_night,
	COUNT(T.address_tuesday_night) as address_tuesday_night,
	
	SUM(T.bottles_wednesday_day) as bottles_wednesday_day,
	COUNT(T.address_wednesday_day) as address_wednesday_day,
	SUM(T.bottles_wednesday_night) as bottles_wednesday_night,
	COUNT(T.address_wednesday_night) as address_wednesday_night,
	
	SUM(T.bottles_thursday_day) as bottles_thursday_day,
	COUNT(T.address_thursday_day) as address_thursday_day,
	SUM(T.bottles_thursday_night) as bottles_thursday_night,
	COUNT(T.address_thursday_night) as address_thursday_night,
	
	SUM(T.bottles_friday_day) as bottles_friday_day,
	COUNT(T.address_friday_day) as address_friday_day,
	SUM(T.bottles_friday_night) as bottles_friday_night,
	COUNT(T.address_friday_night) as address_friday_night,
	
	SUM(T.bottles_saturday_day) as bottles_saturday_day,
	COUNT(T.address_saturday_day) as address_saturday_day,
	SUM(T.bottles_saturday_night) as bottles_saturday_night,
	COUNT(T.address_saturday_night) as address_saturday_night,
	
	SUM(T.bottles_sunday_day) as bottles_sunday_day,
	COUNT(T.address_sunday_day) as address_sunday_day,
	SUM(T.bottles_sunday_night) as bottles_sunday_night,
	COUNT(T.address_sunday_night) as address_sunday_night        
FROM
	(SELECT
		week_days.week_begin,
		week_days.week_end,
		
		IF(dayofweek = 0 and daytime = 'day', order_sum.count, 0)  AS bottles_monday_day,
		IF(dayofweek = 0 and daytime = 'day', compiled_address, null)  AS address_monday_day,
		IF(dayofweek = 0 and daytime = 'night', order_sum.count, 0)  AS bottles_monday_night,
		IF(dayofweek = 0 and daytime = 'night', compiled_address, null)  AS address_monday_night,
		
		IF(dayofweek = 1 and daytime = 'day', order_sum.count, 0)  AS bottles_tuesday_day,
		IF(dayofweek = 1 and daytime = 'day', compiled_address, null)  AS address_tuesday_day,
		IF(dayofweek = 1 and daytime = 'night', order_sum.count, 0)  AS bottles_tuesday_night,
		IF(dayofweek = 1 and daytime = 'night', compiled_address, null)  AS address_tuesday_night,
		
		IF(dayofweek = 2 and daytime = 'day', order_sum.count, 0)  AS bottles_wednesday_day,
		IF(dayofweek = 2 and daytime = 'day', compiled_address, null)  AS address_wednesday_day,
		IF(dayofweek = 2 and daytime = 'night', order_sum.count, 0)  AS bottles_wednesday_night,
		IF(dayofweek = 2 and daytime = 'night', compiled_address, null)  AS address_wednesday_night,
		
		IF(dayofweek = 3 and daytime = 'day', order_sum.count, 0)  AS bottles_thursday_day,
		IF(dayofweek = 3 and daytime = 'day', compiled_address, null)  AS address_thursday_day,
		IF(dayofweek = 3 and daytime = 'night', order_sum.count, 0)  AS bottles_thursday_night,
		IF(dayofweek = 3 and daytime = 'night', compiled_address, null)  AS address_thursday_night,
		
		IF(dayofweek = 4 and daytime = 'day', order_sum.count, 0)  AS bottles_friday_day,
		IF(dayofweek = 4 and daytime = 'day', compiled_address, null)  AS address_friday_day,
		IF(dayofweek = 4 and daytime = 'night', order_sum.count, 0)  AS bottles_friday_night,
		IF(dayofweek = 4 and daytime = 'night', compiled_address, null)  AS address_friday_night,
		
		IF(dayofweek = 5 and daytime = 'day', order_sum.count, 0)  AS bottles_saturday_day,
		IF(dayofweek = 5 and daytime = 'day', compiled_address, null)  AS address_saturday_day,
		IF(dayofweek = 5 and daytime = 'night', order_sum.count, 0)  AS bottles_saturday_night,
		IF(dayofweek = 5 and daytime = 'night', compiled_address, null)  AS address_saturday_night,
		
		IF(dayofweek = 6 and daytime = 'day', order_sum.count, 0)  AS bottles_sunday_day,
		IF(dayofweek = 6 and daytime = 'day', compiled_address, null)  AS address_sunday_day,
		IF(dayofweek = 6 and daytime = 'night', order_sum.count, 0)  AS bottles_sunday_night,
		IF(dayofweek = 6 and daytime = 'night', compiled_address, null)  AS address_sunday_night    
	FROM
		orders
		LEFT JOIN
			(
				SELECT 
					orders.id as order_id,
					date_sub(delivery_date, interval (weekday(delivery_date)) day) as week_begin,
					date_add(delivery_date, interval (6-weekday(delivery_date)) day) as week_end,             
					IF(delivery_schedule.from_time &lt; '18:00', 'day', 'night') as daytime,                
					WEEKDAY(delivery_date) as dayofweek
				FROM 
					orders
					LEFT JOIN 
						delivery_schedule ON delivery_schedule.id = orders.delivery_schedule_id
			) as week_days ON week_days.order_id = orders.id
		LEFT JOIN
			(
				SELECT 
					order_items.order_id,
					SUM(order_items.count) AS count
				FROM
					order_items
					LEFT JOIN nomenclature ON order_items.nomenclature_id = nomenclature.id
				WHERE
					nomenclature.category = 'water'
				GROUP BY order_items.order_id
			) AS order_sum ON order_sum.order_id = orders.id
		LEFT JOIN
			delivery_points ON delivery_point_id = delivery_points.id
		LEFT JOIN
			route_list_addresses ON orders.id = route_list_addresses.order_id
		LEFT JOIN
			route_lists ON route_list_addresses.route_list_id = route_lists.id
		LEFT JOIN
			employees ON route_lists.driver_id = employees.id
		LEFT JOIN 
			delivery_schedule ON delivery_schedule.id = orders.delivery_schedule_id
	WHERE
			(delivery_date BETWEEN @start_date AND @end_date)
		AND route_list_addresses.status &lt;&gt; 'Transfered'
		AND (order_status &lt;&gt; 'Canceled' AND order_status &lt;&gt; 'DeliveryCanceled')
		AND NOT orders.self_delivery
		AND delivery_points.city &lt;&gt; 'Семиозерье') as T
GROUP BY week_begin
ORDER BY week_begin</CommandText>
        <QueryParameters>
          <QueryParameter Name="start_date">
            <Value>=Parameters!start_date</Value>
          </QueryParameter>
          <QueryParameter Name="end_date">
            <Value>=Parameters!end_date</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
