  <DataSets>
    <DataSet Name="DataIncome">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
    if(cash_income.cash_income_category_id,
        1,
        0) as CategoryType,
    IFNULL(cash_income_category.name,
            cash_expense_category.name) as Category,
    IFNULL(cash_income.cash_income_category_id,
            cash_income.cash_expense_category_id) as CategoryId,
    cash_income.money as sum,
    cash_income.description
FROM
    cash_income
        LEFT JOIN
    cash_income_category ON cash_income_category.id = cash_income.cash_income_category_id
        LEFT JOIN
    cash_expense_category ON cash_expense_category.id = cash_income.cash_expense_category_id
WHERE
    cash_income.date BETWEEN @startDate AND DATE_ADD(@endDate, INTERVAL 1 DAY)
ORDER BY Category;</CommandText>
        <QueryParameters>
          <QueryParameter Name="startDate">
            <Value>=Parameters!StartDate</Value>
          </QueryParameter>
          <QueryParameter Name="endDate">
            <Value>=Parameters!EndDate</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="CategoryType">
          <DataField>CategoryType</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Category">
          <DataField>Category</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CategoryId">
          <DataField>CategoryId</DataField>
          <rd:TypeName>System.UInt32</rd:TypeName>
        </Field>
        <Field Name="sum">
          <DataField>sum</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="description">
          <DataField>description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataExpense">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
    cash_expense.cash_expense_category_id as CategoryId,
	cash_expense_category.name as Category,
	cash_expense.money as Sum,
    cash_expense.description
FROM
    cash_expense
        LEFT JOIN
    cash_expense_category ON cash_expense_category.id = cash_expense.cash_expense_category_id
WHERE cash_expense.type &lt;&gt; 'Advance' AND
    cash_expense.date BETWEEN @startDate AND DATE_ADD(@endDate, INTERVAL 1 DAY)
ORDER BY Category;</CommandText>
        <QueryParameters>
          <QueryParameter Name="startDate">
            <Value>=Parameters!StartDate</Value>
          </QueryParameter>
          <QueryParameter Name="endDate">
            <Value>=Parameters!EndDate</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="CategoryId">
          <DataField>CategoryId</DataField>
          <rd:TypeName>System.UInt32</rd:TypeName>
        </Field>
        <Field Name="Category">
          <DataField>Category</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="sum">
          <DataField>Sum</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="description">
          <DataField>description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataAdvance">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
    cash_expense.cash_expense_category_id as CategoryId,
	cash_expense_category.name as Category,
	cash_expense.money as Sum,
    cash_expense.description,
    CONCAT_WS('',
            CONCAT(employees.last_name, ' '),
            if(employees.name = '',
                '',
                CONCAT(LEFT(employees.name, 1), '.')),
            if(employees.patronymic = '',
                '',
                CONCAT(LEFT(employees.patronymic, 1), '.'))) as employee
FROM
    cash_expense
        LEFT JOIN
    cash_expense_category ON cash_expense_category.id = cash_expense.cash_expense_category_id
        LEFT JOIN
    employees ON employees.id = cash_expense.employee_id
WHERE cash_expense.type = 'Advance' AND
    cash_expense.date BETWEEN @startDate AND DATE_ADD(@endDate, INTERVAL 1 DAY)
ORDER BY Category, employee;</CommandText>
        <QueryParameters>
          <QueryParameter Name="startDate">
            <Value>=Parameters!StartDate</Value>
          </QueryParameter>
          <QueryParameter Name="endDate">
            <Value>=Parameters!EndDate</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="CategoryId">
          <DataField>CategoryId</DataField>
          <rd:TypeName>System.UInt32</rd:TypeName>
        </Field>
        <Field Name="Category">
          <DataField>Category</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="sum">
          <DataField>Sum</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="description">
          <DataField>description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="employee">
          <DataField>employee</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
