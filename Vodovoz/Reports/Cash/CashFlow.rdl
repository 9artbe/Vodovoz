  <DataSets>
    <DataSet Name="DataIncome">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT IFNULL(cash_income_category.name, cash_expense_category.name) as Category, SUM(cash_income.money) as sum
FROM cash_income
LEFT JOIN cash_income_category ON cash_income_category.id = cash_income.cash_income_category_id
LEFT JOIN cash_expense_category ON cash_expense_category.id = cash_income.cash_expense_category_id
WHERE cash_income.date BETWEEN @startDate AND @endDate
GROUP BY cash_income.cash_income_category_id, cash_income.cash_expense_category_id
ORDER BY Category;</CommandText>
        <QueryParameters>
          <QueryParameter Name="startDate">
            <Value>=Parameters!StartDate</Value>
          </QueryParameter>
          <QueryParameter Name="endDate">
            <Value>=Parameters!EndDate</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="Category">
          <DataField>Category</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="sum">
          <DataField>sum</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataExpense">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
    cash_expense_category.name as Category,
    SUM(cash_expense.money) as Sum
FROM
    cash_expense
        LEFT JOIN
    cash_expense_category ON cash_expense_category.id = cash_expense.cash_expense_category_id
WHERE cash_expense.type &lt;&gt; 'Advance' AND
    cash_expense.date BETWEEN @startDate AND @endDate
GROUP BY cash_expense.cash_expense_category_id
ORDER BY Category;</CommandText>
        <QueryParameters>
          <QueryParameter Name="startDate">
            <Value>=Parameters!StartDate</Value>
          </QueryParameter>
          <QueryParameter Name="endDate">
            <Value>=Parameters!EndDate</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="Category">
          <DataField>Category</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="sum">
          <DataField>Sum</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataAdvance">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
    cash_expense_category.name as Category,
    SUM(cash_expense.money) as Sum
FROM
    cash_expense
        LEFT JOIN
    cash_expense_category ON cash_expense_category.id = cash_expense.cash_expense_category_id
WHERE cash_expense.type = 'Advance' AND
    cash_expense.date BETWEEN @startDate AND @endDate
GROUP BY cash_expense.cash_expense_category_id
ORDER BY Category;</CommandText>
        <QueryParameters>
          <QueryParameter Name="startDate">
            <Value>=Parameters!StartDate</Value>
          </QueryParameter>
          <QueryParameter Name="endDate">
            <Value>=Parameters!EndDate</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields >
        <Field Name="Category">
          <DataField>Category</DataField>
