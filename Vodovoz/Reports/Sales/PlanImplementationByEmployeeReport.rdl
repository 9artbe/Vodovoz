          <VerticalAlign>Middle</VerticalAlign>
        </Style>
        <CanGrow>true</CanGrow>
        <CanShrink>true</CanShrink>
      </Textbox>
    </ReportItems>
  </PageFooter>
  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <Timeout>120</Timeout>
        <CommandText>SELECT 
	o.id AS order_id,
	c.name AS client,
	IFNULL(GROUP_CONCAT(DISTINCT items.nomenclature SEPARATOR ', '), 'Нет заказа воды 19 л.') AS nomenclature,
	IFNULL(SUM(items.count), 0) AS count,
	IFNULL(SUM(items.total_sum), 0) AS total_sum,
	IFNULL(o.bottles_return, 0) AS bottles_to_return,
	IFNULL(IF(o.self_delivery, o.returned_tare, a.bottles_returned),0) + IFNULL(o.bottles_by_stock_actual_count, 0) AS bottles_returned,
	o.order_status != 'Closed' AND a.route_list_id IS NOT NULL AS unclosed_rl,
	o.order_status != 'Closed' AND a.route_list_id IS NULL AS unclosed_not_in_rl,
	CONCAT('МЛ№', rl.id, ', водитель - ',GET_PERSON_NAME_WITH_INITIALS(e.last_name, e.name, e.patronymic)) AS rl_details
FROM orders o
LEFT JOIN (
	SELECT
		o.id AS order_id,
		GET_SHORT_OR_REGULAR_NAME_FOR_NOMENCLATURE(n.id) AS nomenclature,
		IFNULL(i.actual_count, i.count) AS count,
		GET_TOTAL_SUM_FOR_ORDER_ITEM(i.id) AS total_sum,
		n.id AS nom_id
	FROM orders o
	LEFT JOIN order_items i ON o.id = i.order_id
	LEFT JOIN nomenclature n ON n.id = i.nomenclature_id
	WHERE o.create_date BETWEEN @start_date AND @end_date
		AND o.order_status NOT IN('Canceled', 'DeliveryCanceled', 'NotDelivered')
		AND o.author_employee_id = @employee_id
		AND n.category = 'water' AND n.tare_volume = 'Vol19L'
) AS items ON o.id = items.order_id
LEFT JOIN route_list_addresses a ON a.order_id = o.id
JOIN counterparty c ON c.id = o.client_id
LEFT JOIN route_lists rl ON a.route_list_id = rl.id
LEFT JOIN employees e ON e.id = rl.driver_id
WHERE o.create_date BETWEEN @start_date AND @end_date
	AND o.order_status NOT IN('Canceled', 'DeliveryCanceled', 'NotDelivered')
	AND o.author_employee_id = @employee_id
	AND (items.nomenclature IS NOT NULL OR a.bottles_returned &gt; 0 OR o.bottles_return &gt; 0)
GROUP BY o.id
;</CommandText>
        <QueryParameters>
          <QueryParameter Name="start_date">
            <Value>=Parameters!start_date</Value>
          </QueryParameter>
          <QueryParameter Name="end_date">
            <Value>=Parameters!end_date</Value>
          </QueryParameter>
          <QueryParameter Name="employee_id">
            <Value>={?employee_id}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="order_id">
          <DataField>order_id</DataField>
          <TypeName>System.UInt32</TypeName>
        </Field>
        <Field Name="client">
          <DataField>client</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="nomenclature">
          <DataField>nomenclature</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="count">
          <DataField>count</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="total_sum">
          <DataField>total_sum</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="bottles_to_return">
          <DataField>bottles_to_return</DataField>
          <TypeName>System.Int64</TypeName>
        </Field>
        <Field Name="bottles_returned">
          <DataField>bottles_returned</DataField>
          <TypeName>System.Int64</TypeName>
        </Field>
        <Field Name="unclosed_rl">
          <DataField>unclosed_rl</DataField>
          <TypeName>System.Int64</TypeName>
        </Field>
        <Field Name="unclosed_not_in_rl">
          <DataField>unclosed_not_in_rl</DataField>
          <TypeName>System.Int64</TypeName>
        </Field>
        <Field Name="rl_details">
          <DataField>rl_details</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Employee">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <Timeout>30</Timeout>
        <CommandText>SELECT
	GET_PERSON_NAME_WITH_INITIALS(e.last_name, e.name, e.patronymic) AS name,
	w.full_quantity AS full_plan,
	w.empty_quantity AS empty_plan
FROM employees e
LEFT JOIN wage_parameters w ON w.id = wage_calculation_parameter_id
WHERE e.id = @employee_id
;</CommandText>
        <QueryParameters>
          <QueryParameter Name="@employee_id">
            <Value>={?employee_id}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="name">
          <DataField>name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="full_plan">
          <DataField>full_plan</DataField>
          <TypeName>System.Int32</TypeName>
        </Field>
        <Field Name="empty_plan">
          <DataField>empty_plan</DataField>
          <TypeName>System.Int32</TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
