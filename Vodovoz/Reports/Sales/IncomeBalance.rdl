  <DataSets>
    <DataSet Name="Income">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
	t.*,
    (t.FactIncome - t.PlanedIncome - t.PhoneSum - t.NedovozSum - IF(t.TransferedSum &lt; 0, t.TransferedSum, 0) - t.DepositsSum) as balance
FROM
	(SELECT    
		route_lists.id as number,
		route_lists.date as date,
		employees.last_name as driver,
        #Фактический приход
			(
				SUM(
					CAST(IFNULL(money_movement_operations.money, 0) as DECIMAL(10,2)) * IF(route_list_addresses.status='Completed',1,0)
					+ CAST(IFNULL(deposit_operations.received_deposit, 0) as DECIMAL(10,2))
                    			)                
				+ CAST(IFNULL(PhoneSums.Sum, 0) as DECIMAL(10,2))
			) as FactIncome,
        SUM(Orders.TotalOrdersSum) as PlanedIncome,
		CAST(IFNULL(PhoneSums.Sum, 0) as DECIMAL(10,2)) as PhoneSum,
		IFNULL(nedovoz.nedovoz_sum, 0) as NedovozSum,
		IFNULL(Transfered.TotalOrdersSum, 0) as TransferedSum,
		SUM(IFNULL(deposit_operations.received_deposit, 0)) as DepositsSum,
		CI.CashIncomeDates,
		CAST((IFNULL(CI.MoneyIncome,0)-IFNULL(CE.MoneyExpence,0)) as DECIMAL(10,2)) as IncMinusExp
	FROM
		route_lists
		LEFT JOIN employees ON route_lists.driver_id = employees.id
		LEFT JOIN route_list_addresses ON route_list_addresses.route_list_id = route_lists.id   
		LEFT JOIN orders ON route_list_addresses.order_id = orders.id
		LEFT JOIN money_movement_operations ON money_movement_operations.order_id=orders.id
		LEFT JOIN deposit_operations ON deposit_operations.order_id=orders.id
	#Мобильная связь
		LEFT JOIN
			(
			SELECT 
				T.route_list_id,
			0 - COUNT(T.qty_addr)  * 2 as Sum # 2 рубля за каждый доставленный адрес 
        			FROM(
				SELECT
					route_list_addresses.route_list_id,
					Count(route_list_addresses.id) as qty_addr
				FROM	route_list_addresses
				LEFT JOIN	route_lists ON route_list_addresses.route_list_id = route_lists.id
               				LEFT JOIN	orders ON route_list_addresses.order_id = orders.id
				WHERE 		route_list_addresses.status = 'Completed'
                			GROUP BY	route_list_addresses.route_list_id, orders.delivery_point_id
               			) as T
			GROUP BY T.route_list_id
		) as PhoneSums ON PhoneSums.route_list_id = route_lists.id
	#Плановый приход
		LEFT JOIN
			(
				SELECT 
					orders.id as order_id,
					IFNULL(ROUND(SUM(order_items.price * order_items.count * (1 - (order_items.discount / 100))),2),0) as TotalOrdersSum
				FROM orders
					LEFT JOIN order_items ON order_items.order_id = orders.id AND orders.payment_type = 'cash'
				GROUP BY orders.id
			) as Orders ON Orders.order_id = route_list_addresses.order_id
	#Недовозы
		LEFT JOIN
			(
				SELECT
					route_list_addresses.route_list_id,
					0 - SUM(order_items.price * (order_items.count - order_items.actual_count)) as nedovoz_sum
				FROM
					route_list_addresses
					LEFT JOIN orders ON orders.id = route_list_addresses.order_id
					LEFT JOIN order_items ON order_items.order_id = orders.id
					LEFT JOIN nomenclature ON order_items.nomenclature_id = nomenclature.id
					LEFT JOIN order_equipment ON order_equipment.order_id = orders.id
				WHERE
					orders.payment_type = 'cash'
					AND order_items.price &gt; 0
					AND(
						(
							#Полностью недовезенные заказы без перенесенных адресов (чтобы не засчиталось дважды)
							orders.order_status in ('Canceled', 'NotDelivered', 'DeliveryCanceled') 
							AND route_list_addresses.status != 'transfered'
						)
						OR (
							# Частичные недовозы
							route_list_addresses.status = 'Completed' 
							AND(					
								(
									nomenclature.category in ('water', 'equipment', 'disposableBottleWater', 'additional', 'bottle')
									AND nomenclature.serial = false					
									AND order_items.count != order_items.actual_count
								)
								OR order_equipment.confirmed = false
							)
						)
					)
				GROUP BY route_list_addresses.route_list_id
			) as nedovoz ON nedovoz.route_list_id = route_lists.id
	#Переносы
		LEFT JOIN
			(
				SELECT
					internal_trans.route_list_id,
					SUM(internal_trans.TotalOrdersSum) as TotalOrdersSum
				FROM
					(SELECT
						route_list_addresses.id,
						route_list_addresses.route_list_id,
						route_list_addresses.status,
						ROUND(
							IFNULL(
								IF(status = 'transfered',
								0 - order_items.price * order_items.count * (1 - (order_items.discount / 100)),
								order_items.price * order_items.count * (1 - (order_items.discount / 100))
								)
							, 0)
						,2) as TotalOrdersSum
					FROM 
						route_list_addresses 
						LEFT JOIN orders On orders.id = route_list_addresses.order_id
						LEFT JOIN order_items ON order_items.order_id = orders.id AND orders.payment_type = 'cash'
					WHERE
						(status = 'transfered' 
						OR was_transfered = true)
					) as internal_trans
				GROUP BY internal_trans.route_list_id HAVING TotalOrdersSum != 0
			) as Transfered ON Transfered.route_list_id = route_lists.id
	#Приходные ордера
		LEFT JOIN (
				SELECT 
					route_list_id,
                				SUM(IFNULL(cash_income.money, 0)) as MoneyIncome,
					GROUP_CONCAT(DISTINCT DATE_FORMAT(cash_income.date, '%d.%m.%Y' ) SEPARATOR ', ') as CashIncomeDates
				FROM cash_income
				GROUP BY
					route_list_id
			) AS CI ON CI.route_list_id = route_lists.id
	#Расходные ордера
		LEFT JOIN (
				SELECT 
					route_list_id,
                				SUM(IFNULL(cash_expense.money, 0)) as MoneyExpence
				FROM cash_expense
				GROUP BY
					route_list_id
			) AS CE ON CE.route_list_id = route_lists.id
	WHERE
		route_lists.status IN ('Closed', 'MileageCheck')
		AND route_lists.date &gt;= @StartDate AND route_lists.date &lt;= @EndDate
	GROUP BY route_lists.id
    ) as t</CommandText>
        <QueryParameters>
          <QueryParameter Name="StartDate">
            <Value>=Parameters!StartDate</Value>
          </QueryParameter>
          <QueryParameter Name="EndDate">
            <Value>=Parameters!EndDate</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields >
        <Field Name="number">
          <DataField>number</DataField>
          <TypeName>System.UInt32</TypeName>
        </Field>
        <Field Name="date">
          <DataField>date</DataField>
          <TypeName>System.DateTime</TypeName>
        </Field>
        <Field Name="driver">
          <DataField>driver</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="FactIncome">
          <DataField>FactIncome</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="PlanedIncome">
          <DataField>PlanedIncome</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="PhoneSum">
          <DataField>PhoneSum</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="NedovozSum">
          <DataField>NedovozSum</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="TransferedSum">
          <DataField>TransferedSum</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="DepositsSum">
          <DataField>DepositsSum</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="CashIncomeDates">
          <DataField>CashIncomeDates</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="IncMinusExp">
          <DataField>IncMinusExp</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="balance">
          <DataField>balance</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="IncomeAllStatus">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT    
	route_lists.id as number,
	route_lists.date as date,
	employees.last_name as driver,
    (
		CASE
			WHEN route_lists.status = 'New' THEN 'Новый'
            WHEN route_lists.status = 'InLoading' THEN 'На погрузке'
            WHEN route_lists.status = 'EnRoute' THEN 'В пути'
            WHEN route_lists.status = 'OnClosing' THEN 'Сдаётся'
            ELSE route_lists.status
		END
    )
    as status,
	SUM(Orders.TotalOrdersSum) as BasePlanedIncome,
	SUM(Orders.TotalOrdersSum) + IFNULL(nedovoz.nedovoz_sum, 0) + IFNULL(Transfered.TotalOrdersSum, 0) as CurrenctPlanedIncome,
	IFNULL(nedovoz.nedovoz_sum, 0) as NedovozSum,
	IFNULL(Transfered.TotalOrdersSum, 0) as TransferedSum,
    GROUP_CONCAT(DISTINCT DATE_FORMAT(cash_income.date, '%d.%m.%Y' ) SEPARATOR ', ') as CashIncomeDates
FROM
	route_lists
	LEFT JOIN employees ON route_lists.driver_id = employees.id
	LEFT JOIN route_list_addresses ON route_list_addresses.route_list_id = route_lists.id   
#Мобильная связь
	LEFT JOIN
		(
			SELECT 
				route_list_addresses.route_list_id, 
				0 - (Count(route_list_addresses.id) * 2) as Sum # 2 рубля за каждый доставленный адрес 
			FROM route_list_addresses
				LEFT JOIN route_lists ON route_list_addresses.route_list_id = route_lists.id
			WHERE 
				route_list_addresses.status = 'Completed' 
				|| (route_list_addresses.status = 'EnRoute' &amp;&amp; route_lists.status in ('OnClosing', 'MileageCheck', 'Closed'))
			GROUP BY route_list_addresses.route_list_id
		) as PhoneSums ON PhoneSums.route_list_id = route_lists.id
#Плановый приход
	LEFT JOIN
		(
			SELECT 
				orders.id as order_id,
				IFNULL(ROUND(SUM(order_items.price * order_items.count * (1 - (order_items.discount / 100))),2),0) as TotalOrdersSum
			FROM orders
				LEFT JOIN order_items ON order_items.order_id = orders.id AND orders.payment_type = 'cash'
			GROUP BY orders.id
		) as Orders ON Orders.order_id = route_list_addresses.order_id
#Недовозы
	LEFT JOIN
		(
			SELECT
				route_list_addresses.route_list_id,
				0 - SUM(order_items.price * (order_items.count - order_items.actual_count)) as nedovoz_sum
			FROM
				route_list_addresses
				LEFT JOIN orders ON orders.id = route_list_addresses.order_id
				LEFT JOIN order_items ON order_items.order_id = orders.id
				LEFT JOIN nomenclature ON order_items.nomenclature_id = nomenclature.id
				LEFT JOIN order_equipment ON order_equipment.order_id = orders.id
			WHERE
				orders.payment_type = 'cash'
					AND order_items.price &gt; 0
					AND	(
							#Полностью недовезенные заказы без перенесенных адресов (чтобы не засчиталось дважды)
							orders.order_status in ('Canceled', 'NotDelivered', 'DeliveryCanceled') 
							AND route_list_addresses.status != 'transfered'
					)					
			GROUP BY route_list_addresses.route_list_id
		) as nedovoz ON nedovoz.route_list_id = route_lists.id
#Переносы
	LEFT JOIN
		(
			SELECT
				internal_trans.route_list_id,
				SUM(internal_trans.TotalOrdersSum) as TotalOrdersSum
			FROM
				(SELECT
					route_list_addresses.id,
					route_list_addresses.route_list_id,
					route_list_addresses.status,
					ROUND(
						IFNULL(
							IF(status = 'transfered',
							0 - order_items.price * order_items.count * (1 - (order_items.discount / 100)),
							order_items.price * order_items.count * (1 - (order_items.discount / 100))
							)
						, 0)
					,2) as TotalOrdersSum
				FROM 
					route_list_addresses 
					LEFT JOIN orders On orders.id = route_list_addresses.order_id
					LEFT JOIN order_items ON order_items.order_id = orders.id AND orders.payment_type = 'cash'
				WHERE
					(status = 'transfered' 
					OR was_transfered = true)
				) as internal_trans
			GROUP BY internal_trans.route_list_id HAVING TotalOrdersSum != 0
		) as Transfered ON Transfered.route_list_id = route_lists.id
#Приходные ордера
	LEFT JOIN cash_income ON cash_income.route_list_id = route_lists.id	
WHERE
	route_lists.status NOT IN ('Closed', 'MileageCheck')
