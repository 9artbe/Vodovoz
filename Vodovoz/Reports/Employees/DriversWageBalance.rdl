  <DataSets>
    <DataSet Name="DriverBalance">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
	CONCAT_WS(' ', employees.last_name, employees.name) AS driver,
	SUM(wages_movement_operations.money) AS balance,
	employees.category
FROM wages_movement_operations
LEFT JOIN employees ON employees.id = wages_movement_operations.employee_id
LEFT JOIN wage_parameters w ON w.employee_id = employees.id
LEFT JOIN cars ON employees.id = cars.driver_id
WHERE
	wages_movement_operations.operation_time &lt;= @date
	AND wages_movement_operations.employee_id IN (@drivers)
	AND employees.category = 'driver'
	AND w.wage_type != 'Manual'
	AND (employees.driver_of != 'CompanyLargus' OR driver_of IS NULL)
	AND (employees.driver_of != 'CompanyGAZelle' OR driver_of IS NULL)
	AND (cars.is_raskat = false OR cars.is_raskat IS NULL)
GROUP BY wages_movement_operations.employee_id
HAVING balance &lt;&gt; 0
ORDER BY driver
;</CommandText>
        <QueryParameters>
          <QueryParameter Name="date">
            <Value>=Parameters!date</Value>
          </QueryParameter>
          <QueryParameter Name="drivers">
            <Value>=Parameters!drivers</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields >
        <Field Name="driver">
          <DataField>driver</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="balance">
          <DataField>balance</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="category">
          <DataField>category</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
    </DataSet>
