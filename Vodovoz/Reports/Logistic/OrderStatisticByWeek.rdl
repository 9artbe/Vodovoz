  <DataSets>
    <DataSet Name="Data">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    T.week_begin,
    T.week_end,
    SUM(T.bottles_monday) as bottles_monday,
    COUNT(T.address_monday) as address_monday,

    SUM(T.bottles_tuesday) as bottles_tuesday,
    COUNT(T.address_tuesday) as address_tuesday,

    SUM(T.bottles_wednesday) as bottles_wednesday,
    COUNT(T.address_wednesday) as address_wednesday,

    SUM(T.bottles_thursday) as bottles_thursday,
    COUNT(T.address_thursday) as address_thursday,

    SUM(T.bottles_friday) as bottles_friday,
    COUNT(T.address_friday) as address_friday,

    SUM(T.bottles_saturday) as bottles_saturday,
    COUNT(T.address_saturday) as address_saturday,

    SUM(T.bottles_sunday) as bottles_sunday,
    COUNT(T.address_sunday) as address_sunday
FROM
    (SELECT
         week_days.week_begin,
         week_days.week_end,

         IF(dayofweek = 0, order_sum.count, 0)  AS bottles_monday,
         IF(dayofweek = 0, compiled_address, null)  AS address_monday,

         IF(dayofweek = 1 , order_sum.count, 0)  AS bottles_tuesday,
         IF(dayofweek = 1 , compiled_address, null)  AS address_tuesday,

         IF(dayofweek = 2 , order_sum.count, 0)  AS bottles_wednesday,
         IF(dayofweek = 2 , compiled_address, null)  AS address_wednesday,

         IF(dayofweek = 3 , order_sum.count, 0)  AS bottles_thursday,
         IF(dayofweek = 3 , compiled_address, null)  AS address_thursday,

         IF(dayofweek = 4 , order_sum.count, 0)  AS bottles_friday,
         IF(dayofweek = 4 , compiled_address, null)  AS address_friday,

         IF(dayofweek = 5 , order_sum.count, 0)  AS bottles_saturday,
         IF(dayofweek = 5 , compiled_address, null)  AS address_saturday,

         IF(dayofweek = 6 , order_sum.count, 0)  AS bottles_sunday,
         IF(dayofweek = 6 , compiled_address, null)  AS address_sunday
     FROM
         orders
             LEFT JOIN
         (
             SELECT
                 orders.id as order_id,
                 date_sub(delivery_date, interval (weekday(delivery_date)) day) as week_begin,
                 date_add(delivery_date, interval (6-weekday(delivery_date)) day) as week_end,
                 WEEKDAY(delivery_date) as dayofweek
             FROM
                 orders
                     LEFT JOIN
                 delivery_schedule ON delivery_schedule.id = orders.delivery_schedule_id
         ) as week_days ON week_days.order_id = orders.id
             LEFT JOIN
         (
             SELECT
                 order_items.order_id,
                 SUM(order_items.count) AS count
             FROM
                 order_items
                     LEFT JOIN nomenclature ON order_items.nomenclature_id = nomenclature.id
             WHERE
                     nomenclature.category = 'water' AND NOT nomenclature.is_disposable_tare
             GROUP BY order_items.order_id
         ) AS order_sum ON order_sum.order_id = orders.id
             LEFT JOIN
         delivery_points ON delivery_point_id = delivery_points.id
             LEFT JOIN
         route_list_addresses ON orders.id = route_list_addresses.order_id
             LEFT JOIN
         route_lists ON route_list_addresses.route_list_id = route_lists.id
             LEFT JOIN
         employees ON route_lists.driver_id = employees.id
             LEFT JOIN
         delivery_schedule ON delivery_schedule.id = orders.delivery_schedule_id
     WHERE
         (delivery_date BETWEEN @start_date AND @end_date)
       AND route_list_addresses.status &lt;&gt; 'Transfered'
       AND (order_status &lt;&gt; 'Canceled' AND order_status &lt;&gt; 'DeliveryCanceled' AND order_status != 'WaitForPayment')
       AND NOT orders.self_delivery
       AND orders.is_contract_closer = 0
       AND orders.service = 0) as T
GROUP BY week_begin
ORDER BY week_begin
</CommandText>
        <QueryParameters>
          <QueryParameter Name="start_date">
            <Value>=Parameters!start_date</Value>
          </QueryParameter>
          <QueryParameter Name="end_date">
            <Value>=Parameters!end_date</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
