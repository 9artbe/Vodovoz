    <DataSet Name="k_nedovozam">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    count(*) as nedovozi
FROM
    undelivered_orders undeliv_orders
        left outer join
    guilty_in_undelivered_orders guiltyinun
    on undeliv_orders.id=guiltyinun.undelivery_id
        left outer join
    subdivisions subdivisio
    on guiltyinun.guilty_department_id=subdivisio.id
        left outer join
    orders oldorderal
    on undeliv_orders.old_order_id=oldorderal.id
WHERE
        oldorderal.delivery_date &gt;= @start_date
        and oldorderal.delivery_date &lt;= @end_date
        and guiltyinun.guilty_side = 'Driver' 
        and undeliv_orders.status = 'Closed'
        and (subdivisio.id = 13 or subdivisio.id = 26);</CommandText>
        <QueryParameters>
          <QueryParameter Name="start_date">
            <Value>=Parameters!start_date</Value>
          </QueryParameter>
          <QueryParameter Name="end_date">
            <Value>=Parameters!end_date</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields >
