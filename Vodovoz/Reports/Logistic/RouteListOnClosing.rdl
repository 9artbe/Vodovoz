        </Field>
        <Field Name="closing_comment">
          <DataField>closing_comment</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="paytelephone">
          <DataField>paytelephone</DataField>
          <TypeName>System.Int16</TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
