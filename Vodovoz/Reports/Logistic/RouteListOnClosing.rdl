  <DataSets>
    <DataSet Name="RouteLists">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
    route_lists.id AS routeListId,
    route_lists.date AS routeListDate,
    route_lists.driver_id,
    CONCAT_WS(' ',
            employees.last_name,
            IF(employees.name = '',
                NULL,
                employees.name),
            IF(employees.patronymic = '',
                NULL,
                employees.patronymic)) AS driver_name,
    IFNULL( (SELECT 
            SUM(total_cash) -  IFNULL(money,0) + SUM(deposits_collected) + SUM(equipment_deposits_collected)
        FROM
            route_list_addresses
        WHERE
            route_list_addresses.status = 'Completed' AND
            route_list_addresses.route_list_id = routeListId), 0) AS nal,
    IFNULL((SELECT 
             count(*)*2
        FROM
            route_list_addresses
        LEFT JOIN
            route_lists ON route_list_addresses.route_list_id = route_lists.id
        LEFT JOIN
            cars ON cars.id = route_lists.car_id
        WHERE
           (is_company_havings = 0 OR (is_company_havings = 1 AND cars.type_of_use &lt;&gt; 'Truck')) AND
            route_list_addresses.status = 'Completed' AND
            route_list_addresses.route_list_id = routeListId), 0)  AS paytelephone,
           closing_comment
FROM
    route_lists
        LEFT JOIN
    employees ON employees.id = route_lists.driver_id
        LEFT JOIN
    route_list_addresses ON route_list_addresses.route_list_id = route_lists.id
        LEFT JOIN
    orders ON route_list_addresses.order_id = orders.id
        LEFT JOIN
    cash_income ON cash_income.route_list_id = route_list_addresses.route_list_id
WHERE
    route_lists.status = 'OnClosing'
        AND ((route_lists.date &lt; CURDATE()
        AND @todayRloff = 1)
        OR (@todayRloff = 0))
GROUP BY routeListId</CommandText>
        <QueryParameters>
          <QueryParameter Name="@todayRloff">
            <Value>={?todayRloff}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="routeListId">
          <DataField>routeListId</DataField>
          <TypeName>System.Int32</TypeName>
        </Field>
        <Field Name="routeListDate">
          <DataField>routeListDate</DataField>
          <TypeName>System.DateTime</TypeName>
        </Field>
        <Field Name="driver_name">
          <DataField>driver_name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="nal">
          <DataField>nal</DataField>
