  <DataSets>
    <DataSet Name="Data">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    orders.id AS order_id,
    orders.delivery_date AS delivery_date,
    counterparty.name AS client_name,
    orders.sum_to_receive ,
    (
        SELECT GROUP_CONCAT(DISTINCT discount_reasons.name SEPARATOR ', ')
        FROM order_items
        JOIN discount_reasons ON order_items.discount_reason_id = discount_reasons.id
        WHERE order_items.order_id = orders.id
    ) AS discount,
    SecondOrder.delivery_date AS secondOrderDate,
    SecondOrder.Id AS secondOrderId ,
    Bottles.Balance as BottleBalance,
    IFNULL(
        (SELECT TRIM('\n' FROM GROUP_CONCAT(DISTINCT CONCAT('8', digits_number) SEPARATOR ';\n'))
        FROM phones
        WHERE delivery_point_id = orders.delivery_point_id),
        (SELECT TRIM('\n' FROM GROUP_CONCAT(DISTINCT CONCAT('8', digits_number) SEPARATOR ';\n'))
        FROM phones
        WHERE counterparty_id = orders.client_id)
    ) AS phones
FROM 
    orders
    JOIN order_items ON orders.id = order_items.order_id
    JOIN discount_reasons ON order_items.discount_reason_id = discount_reasons.id
    JOIN counterparty ON orders.id = counterparty.first_order_id
    LEFT JOIN phones p1 ON p1.counterparty_id = orders.client_id
    LEFT JOIN phones p2 ON p2.delivery_point_id = orders.delivery_point_id
    
    LEFT JOIN 
    (
        SELECT 
            orders.delivery_date ,
            orders.client_id ,
            orders.id as Id
        FROM orders
        LEFT JOIN counterparty ON counterparty.id = orders.client_id
        WHERE counterparty.first_order_id != orders.id
            AND orders.order_status = 'Closed'
        ORDER BY orders.id DESC
    ) as SecondOrder ON SecondOrder.client_id = orders.client_id
    
    LEFT JOIN
    (
        SELECT
            bottles_movement_operations.counterparty_id,
            SUM((CAST(bottles_movement_operations.delivered AS SIGNED) - CAST(bottles_movement_operations.returned AS SIGNED))) as Balance
        FROM
            bottles_movement_operations
        GROUP BY
            bottles_movement_operations.counterparty_id
    ) AS Bottles ON Bottles.counterparty_id = orders.client_id
    
WHERE 
    orders.delivery_date BETWEEN @start_date AND @end_date AND
    orders.order_status = 'Closed' AND
    (@discount_id = 0 OR discount_reasons.id = @discount_id) AND
    (@show_only_client_wiht_one_order = 0 OR SecondOrder.Id IS NULL)
GROUP BY 
    orders.id DESC;</CommandText>
        <QueryParameters>
          <QueryParameter Name="start_date">
            <Value>=Parameters!start_date</Value>
          </QueryParameter>
          <QueryParameter Name="end_date">
            <Value>=Parameters!end_date</Value>
          </QueryParameter>
          <QueryParameter Name="show_only_client_wiht_one_order">
            <Value>={?show_only_client_wiht_one_order}</Value>
          </QueryParameter>
          <QueryParameter Name="discount_id">
            <Value>={?discount_id}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields >
        <Field Name="order_id">
          <DataField>order_id</DataField>
          <TypeName>System.UInt32</TypeName>
        </Field>
        <Field Name="delivery_date">
          <DataField>delivery_date</DataField>
          <TypeName>System.DateTime</TypeName>
        </Field>
        <Field Name="client_name">
          <DataField>client_name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="sum_to_receive">
          <DataField>sum_to_receive</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="discount">
          <DataField>discount</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="secondOrderDate">
          <DataField>secondOrderDate</DataField>
          <TypeName>System.DateTime</TypeName>
        </Field>
        <Field Name="secondOrderId">
          <DataField>secondOrderId</DataField>
          <TypeName>System.UInt32</TypeName>
        </Field>
        <Field Name="BottleBalance">
          <DataField>BottleBalance</DataField>
          <TypeName>System.Int64</TypeName>
        </Field>
        <Field Name="phones">
          <DataField>phones</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
    </DataSet>
