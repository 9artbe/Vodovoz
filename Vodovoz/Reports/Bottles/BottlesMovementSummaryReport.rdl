    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
    byOrders.*,
    (SELECT 
            COUNT(*) = 0
        FROM
            route_lists
        WHERE
            route_lists.date = byOrders.date
                AND route_lists.status &lt;&gt; 'Closed') AS all_closed,
    (SELECT 
            IFNULL(SUM(warehouse_movement_operations.amount), 0)
        FROM
            route_lists
                INNER JOIN
            store_car_load_documents ON store_car_load_documents.route_list_id = route_lists.id
                INNER JOIN
            store_car_load_document_items ON store_car_load_document_items.car_load_document_id = store_car_load_documents.id
                INNER JOIN
            warehouse_movement_operations ON store_car_load_document_items.warehouse_movement_operation_id = warehouse_movement_operations.id
                INNER JOIN
            nomenclature ON warehouse_movement_operations.nomenclature_id = nomenclature.id
        WHERE
            route_lists.date = byOrders.date
                AND (nomenclature.category = 'water'
                OR nomenclature.category = 'bottle')
                AND nomenclature.is_new_bottle = 0
                AND nomenclature.is_defective_bottle = 0) AS car_load_count,
    (SELECT 
            IFNULL(SUM(warehouse_movement_operations.amount), 0)
        FROM
            route_lists
                INNER JOIN
            store_car_unload_documents ON store_car_unload_documents.route_list_id = route_lists.id
                INNER JOIN
            store_car_unload_document_items ON store_car_unload_document_items.car_unload_document_id = store_car_unload_documents.id
                INNER JOIN
            warehouse_movement_operations ON store_car_unload_document_items.warehouse_movement_operation_id = warehouse_movement_operations.id
                INNER JOIN
            nomenclature ON warehouse_movement_operations.nomenclature_id = nomenclature.id
        WHERE
            route_lists.date = byOrders.date
                AND (nomenclature.category = 'water'
                OR nomenclature.category = 'bottle')
                AND nomenclature.is_new_bottle = 0
                AND nomenclature.is_defective_bottle = 0) AS car_unload_count,
    (SELECT 
            IFNULL(SUM(warehouse_movement_operations.amount),
                        0)
        FROM
            route_lists
                INNER JOIN
            store_car_unload_documents ON store_car_unload_documents.route_list_id = route_lists.id
                INNER JOIN
            store_car_unload_document_items ON store_car_unload_document_items.car_unload_document_id = store_car_unload_documents.id
                INNER JOIN
            warehouse_movement_operations ON store_car_unload_document_items.warehouse_movement_operation_id = warehouse_movement_operations.id
                INNER JOIN
            nomenclature ON warehouse_movement_operations.nomenclature_id = nomenclature.id
        WHERE
            route_lists.date = byOrders.date
                AND nomenclature.category = 'bottle'
                AND nomenclature.is_defective_bottle = 1) + (SELECT 
            IFNULL(SUM(warehouse_movement_operations.amount),
                        0)
        FROM
            store_incoming_invoice
                INNER JOIN
            store_incoming_invoice_items ON store_incoming_invoice_items.incoming_invoice_id = store_incoming_invoice.id
                INNER JOIN
            warehouse_movement_operations ON store_incoming_invoice_items.good_move_operation_id = warehouse_movement_operations.id
                INNER JOIN
            nomenclature ON warehouse_movement_operations.nomenclature_id = nomenclature.id
        WHERE
            store_incoming_invoice.time_stamp &gt;= byOrders.date
                AND store_incoming_invoice.time_stamp &lt; DATE_ADD(byOrders.date, INTERVAL 1 DAY)
                AND nomenclature.category = 'bottle'
                AND nomenclature.is_defective_bottle = 1) + (SELECT 
            IFNULL(SUM(warehouse_movement_operations.amount),
                        0)
        FROM
            store_inventory
                INNER JOIN
            store_inventory_item ON store_inventory_item.store_inventory_id = store_inventory.id
                INNER JOIN
            warehouse_movement_operations ON store_inventory_item.warehouse_movement_operation_id = warehouse_movement_operations.id
                INNER JOIN
            nomenclature ON warehouse_movement_operations.nomenclature_id = nomenclature.id
        WHERE
            store_inventory.time_stamp &gt;= byOrders.date
                AND store_inventory.time_stamp &lt; DATE_ADD(byOrders.date, INTERVAL 1 DAY)
                AND nomenclature.category = 'bottle'
                AND nomenclature.is_defective_bottle = 1) + (SELECT 
            IFNULL(SUM(warehouse_movement_operations.amount),
                        0)
        FROM
            store_regrading_of_goods
                INNER JOIN
            store_regrading_of_goods_items ON store_regrading_of_goods_items.store_regrading_of_goods_id = store_regrading_of_goods.id
                INNER JOIN
            warehouse_movement_operations ON store_regrading_of_goods_items.warehouse_income_operation_id = warehouse_movement_operations.id
                INNER JOIN
            nomenclature ON warehouse_movement_operations.nomenclature_id = nomenclature.id
        WHERE
            store_regrading_of_goods.time_stamp &gt;= byOrders.date
                AND store_regrading_of_goods.time_stamp &lt; DATE_ADD(byOrders.date, INTERVAL 1 DAY)
                AND nomenclature.category = 'bottle'
                AND nomenclature.is_defective_bottle = 1) AS defective_count,
    (SELECT 
            IFNULL(SUM(warehouse_movement_operations.amount),
                        0)
        FROM
            route_lists
                INNER JOIN
            store_car_unload_documents ON store_car_unload_documents.route_list_id = route_lists.id
                INNER JOIN
            store_car_unload_document_items ON store_car_unload_document_items.car_unload_document_id = store_car_unload_documents.id
                INNER JOIN
            warehouse_movement_operations ON store_car_unload_document_items.warehouse_movement_operation_id = warehouse_movement_operations.id
                INNER JOIN
            nomenclature ON warehouse_movement_operations.nomenclature_id = nomenclature.id
        WHERE
            route_lists.date = byOrders.date
                AND nomenclature.category = 'bottle'
                AND nomenclature.is_new_bottle = 1) + (SELECT 
            IFNULL(SUM(warehouse_movement_operations.amount),
                        0)
        FROM
            store_incoming_invoice
                INNER JOIN
            store_incoming_invoice_items ON store_incoming_invoice_items.incoming_invoice_id = store_incoming_invoice.id
                INNER JOIN
            warehouse_movement_operations ON store_incoming_invoice_items.good_move_operation_id = warehouse_movement_operations.id
                INNER JOIN
            nomenclature ON warehouse_movement_operations.nomenclature_id = nomenclature.id
        WHERE
            store_incoming_invoice.time_stamp &gt;= byOrders.date
                AND store_incoming_invoice.time_stamp &lt; DATE_ADD(byOrders.date, INTERVAL 1 DAY)
                AND nomenclature.category = 'bottle'
                AND nomenclature.is_new_bottle = 1) AS new_bottle_count
FROM
    (SELECT 
        DATE(bottles_movement_operations.operation_time) AS date,
            SUM(IF(orders.reason_type = 'NewAddress', bottles_movement_operations.delivered, 0)) AS delivered_NewAddress,
            SUM(IF(orders.reason_type = 'NewAddress', bottles_movement_operations.returned, 0)) AS returned_NewAddress,
            SUM(IF(orders.reason_type = 'OrderIncrease', bottles_movement_operations.delivered, 0)) AS delivered_OrderIncrease,
            SUM(IF(orders.reason_type = 'OrderIncrease', bottles_movement_operations.returned, 0)) AS returned_OrderIncrease,
            SUM(IF(orders.reason_type = 'FirstOrder', bottles_movement_operations.delivered, 0)) AS delivered_FirstOrder,
            SUM(IF(orders.reason_type = 'FirstOrder', bottles_movement_operations.returned, 0)) AS returned_FirstOrder,
            SUM(IF(orders.reason_type = 'Unknown', bottles_movement_operations.delivered, 0)) AS delivered_Unknown,
            SUM(IF(orders.reason_type = 'Unknown', bottles_movement_operations.returned, 0)) AS returned_Unknown,
            SUM(bottles_movement_operations.delivered) AS delivered_All,
            SUM(bottles_movement_operations.returned) AS returned_All
    FROM
        bottles_movement_operations
    LEFT JOIN orders ON orders.bottles_movement_operation_id = bottles_movement_operations.id
    WHERE
        bottles_movement_operations.operation_time &gt;= @start_date
            AND bottles_movement_operations.operation_time &lt; DATE_ADD(@end_date, INTERVAL 1 DAY)
    GROUP BY DATE(bottles_movement_operations.operation_time)) byOrders</CommandText>
        <QueryParameters>
          <QueryParameter Name="end_date">
            <Value>=Parameters!end_date</Value>
          </QueryParameter>
          <QueryParameter Name="start_date">
            <Value>=Parameters!start_date</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="date">
          <DataField>date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="delivered_NewAddress">
          <DataField>delivered_NewAddress</DataField>
